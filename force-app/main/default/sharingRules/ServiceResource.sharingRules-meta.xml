<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>PC_FLPs_Service_Resource_Record_Access</fullName>
        <accessLevel>Read</accessLevel>
        <description>Shares PC Service Resource with all user in FLP (PCP) public group. The home page&apos;s controller, ProviderConnectController, requires this access.</description>
        <label>PC FLPs Service Resource Record Access</label>
        <sharedTo>
            <group>PC_Provider_Connect_PCPs</group>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Provider Connect Resource</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>PC_Managers_Service_Resource_Record_Access</fullName>
