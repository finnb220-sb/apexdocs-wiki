        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Date</name>
            <conditionLogic>(1 AND 2 AND 3 AND 4) OR ((1 OR 2) AND (3 OR 4))</conditionLogic>
            <conditions>
                <leftValueReference>progressNoteRecord.VCC_Cannot_Complete_SCD_Action_Reason__c</leftValueReference>
                <operator>Contains</operator>
