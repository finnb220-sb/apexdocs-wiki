        <rules>
            <name>go_to_happy_day</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_e911_Metric.VCC_Street__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
