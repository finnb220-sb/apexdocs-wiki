<?xml version="1.0" encoding="UTF-8" ?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>MPIE_Svc_Appt_Sharing</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Provides access to records for the MPI-E merge related process</description>
        <label>MPI-E Svc Appt Sharing</label>
        <sharedTo>
            <group>MPIE_Integration_Users</group>
        </sharedTo>
        <criteriaItems>
            <field>CreatedById</field>
            <operation>notEqual</operation>
            <value />
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>VAHC_Integration_User_Svc_Appt_Sharing</fullName>
        <accessLevel>Read</accessLevel>
        <description>This sharing rule shares all Service Appointment records that the VAHC Integration User does *not* own with the VAHC Integration User.</description>
        <label>VAHC Integration User Svc Appt Sharing</label>
        <sharedTo>
            <group>VAHC_Integration_User_PG</group>
        </sharedTo>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>notEqual</operation>
            <value />
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
