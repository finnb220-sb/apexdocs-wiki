    <decisions>
        <name>Are_Other_Values_Populated</name>
        <label>Are Other Values Populated?</label>
        <locationX>182</locationX>
        <locationY>1058</locationY>
        <defaultConnector>
            <targetReference>Is_Date_Time_Set_For_Patient_Requests_Appt</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Other</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>progressNoteRecord.VCC_Cannot_Complete_SCD_Action_Reason__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Consult Review Required</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>progressNoteRecord.VCC_Cannot_Complete_SCD_Action_Reason__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Restricted / Unavailable Clinic</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>progressNoteRecord.VCC_Cannot_Complete_SCD_Action_Reason__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>No Appointment Available</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>progressNoteRecord.VCC_Cannot_Complete_SCD_Action_Reason__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Not Authorized in Service Line Agreement</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Cannot_Complete_Appointment_Request</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
