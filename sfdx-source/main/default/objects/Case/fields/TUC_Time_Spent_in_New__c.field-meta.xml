<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TUC_Time_Spent_in_New__c</fullName>
    <description>Time in minutes spent in New</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Status,&quot;New&quot;), ROUND(((NOW() - CreatedDate)*1440),0), TUC_Total_Time_in_New__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Time in minutes spent in New</inlineHelpText>
    <label>Time Spent in New</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
