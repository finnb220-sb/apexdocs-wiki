<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TED_Hour_Created__c</fullName>
    <description>Formula to identify the hour in which a case was created. Used in metrics.</description>
    <externalId>false</externalId>
    <formula>VALUE( MID( TEXT( CreatedDate - (7/24) ), 12, 2 ) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Hour Created</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
