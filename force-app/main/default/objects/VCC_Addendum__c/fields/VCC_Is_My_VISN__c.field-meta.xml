<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VCC_Is_My_VISN__c</fullName>
    <description>Returns true if the VISN for the addendum matches the VISN for the user</description>
    <externalId>false</externalId>
    <formula>IF( VCC_Owner_Division__c = $User.Division ,TRUE, FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is My VISN</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
