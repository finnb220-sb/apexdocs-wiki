<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VCC_Is_Appt_Overdue__c</fullName>
    <description>Checks if the Appointment is overdue and if so, checks the box.</description>
    <externalId>false</externalId>
    <formula>NOT(ISBLANK(VCC_Appt_Due_Date__c)) &amp;&amp; VCC_Appt_Due_Date__c  &lt;  NOW()</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Appt Overdue?</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
