<?xml version="1.0" encoding="UTF-8" ?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>52.0</apiVersion>
    <assignments>
        <name>Assign_to_MSA_Queue_0</name>
        <label>Assign to MSA Queue</label>
        <locationX>842</locationX>
        <locationY>566</locationY>
        <assignmentItems>
            <assignToReference>caseRecord.OwnerId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>MSAQueue.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Owner_0</targetReference>
        </connector>
    </assignments>
    <constants>
        <name>MSAQueueDevName</name>
        <dataType>String</dataType>
        <value>
            <stringValue>CCC_V8_MSA_DROPPED_CALL</stringValue>
        </value>
    </constants>
    <decisions>
        <name>Assign_to_Me_Case_Validation</name>
        <label>Assign to MSA Queue Case Validation</label>
        <locationX>380</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>Get_MSA_Drop_Call_Queue</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Case_is_already_Closed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>caseRecord.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed - Resolved</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>caseRecord.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Closed - Transferred</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Case_is_Closed</targetReference>
            </connector>
            <label>Case is Closed</label>
        </rules>
        <rules>
            <name>Check_current_User</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>caseRecord.OwnerId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>case_owner</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Show_Owner_change_error</targetReference>
            </connector>
            <label>Check current User</label>
        </rules>
    </decisions>
    <decisions>
        <name>CheckMSAQueueName</name>
        <label>CheckMSAQueueName</label>
        <locationX>710</locationX>
        <locationY>458</locationY>
        <defaultConnector>
            <targetReference>Assign_to_MSA_Queue_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>IsMSAQueueAvailable</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>MSAQueue</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>QueueNotAvailable</targetReference>
            </connector>
            <label>IsMSAQueueAvailable</label>
        </rules>
    </decisions>
    <decisions>
        <name>Was_Fault_because_Case_should_not_belong_in_Queue</name>
        <label>Was Fault because Case should not belong in Queue?</label>
        <locationX>1238</locationX>
        <locationY>782</locationY>
        <defaultConnector>
            <targetReference>Flow_Error_Display</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No, Other Issue</defaultConnectorLabel>
        <rules>
            <name>Yes_Case_Should_Not_Be_In_Queue</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Flow.FaultMessage</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>This Queue cannot accept non</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Prevent_Queue_Flow_Error_Screen</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>Assign Case to MSA Queue</description>
    <environments>Default</environments>
    <formulas>
        <name>CalcMSAQueue</name>
        <dataType>String</dataType>
        <expression>&quot;CCC_V&quot; + mid({!$User.Division},6,2) + &quot;_MSA&quot;</expression>
    </formulas>
    <formulas>
        <name>case_owner</name>
        <dataType>String</dataType>
        <expression>CASESAFEID({!$User.Id})</expression>
    </formulas>
    <formulas>
        <description>Gets the 18 digit User ID</description>
        <name>varUserIdFormula</name>
        <dataType>String</dataType>
        <expression>CASESAFEID({!$User.Id})</expression>
    </formulas>
    <interviewLabel>Assign Case to MSA {!$Flow.CurrentDateTime}</interviewLabel>
    <label>VCC Assign Case to MSA</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>Flow</processType>
    <recordLookups>
        <name>Get_MSA_Drop_Call_Queue</name>
        <label>Get MSA Drop Call Queue</label>
        <locationX>710</locationX>
        <locationY>350</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>CheckMSAQueueName</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Type</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Queue</stringValue>
            </value>
        </filters>
        <filters>
            <field>DeveloperName</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>CalcMSAQueue</elementReference>
            </value>
        </filters>
        <object>Group</object>
        <outputReference>MSAQueue</outputReference>
        <queriedFields>Id</queriedFields>
    </recordLookups>
    <recordLookups>
        <name>Get_This_Case</name>
        <label>Get This Case</label>
        <locationX>380</locationX>
        <locationY>134</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Assign_to_Me_Case_Validation</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <object>Case</object>
        <outputReference>caseRecord</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>Status</queriedFields>
        <queriedFields>OwnerId</queriedFields>
    </recordLookups>
    <recordUpdates>
        <name>Update_Owner_0</name>
        <label>Update Owner</label>
        <locationX>842</locationX>
        <locationY>674</locationY>
        <connector>
            <targetReference>Owner_Updated_Successfully_0</targetReference>
        </connector>
        <faultConnector>
            <targetReference>Was_Fault_because_Case_should_not_belong_in_Queue</targetReference>
        </faultConnector>
        <inputReference>caseRecord</inputReference>
    </recordUpdates>
