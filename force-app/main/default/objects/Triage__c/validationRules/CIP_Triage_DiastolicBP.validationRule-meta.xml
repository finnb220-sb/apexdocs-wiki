<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CIP_Triage_DiastolicBP</fullName>
    <active>false</active>
    <description>CIP Triage Diastolic Blood Pressure: should be between 0 and 300</description>
    <errorConditionFormula>Diastolic_Blood_Pressure__c &lt; 0 || Diastolic_Blood_Pressure__c &gt; 300</errorConditionFormula>
    <errorDisplayField>Diastolic_Blood_Pressure__c</errorDisplayField>
    <errorMessage>Please enter a value between 0 and 300</errorMessage>
</ValidationRule>
