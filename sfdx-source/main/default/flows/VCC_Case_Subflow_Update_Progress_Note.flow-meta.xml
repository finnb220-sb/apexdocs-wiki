                <rightValue>
                    <stringValue>Not Authorized in Service Line Agreement</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>progressNoteRecord.VCC_Patient_Requests_Appt_date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>progressNoteRecord.VCC_Time_Preference__c</leftValueReference>
