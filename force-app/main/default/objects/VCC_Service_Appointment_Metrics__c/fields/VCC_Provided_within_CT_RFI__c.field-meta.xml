<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VCC_Provided_within_CT_RFI__c</fullName>
    <description>Returns True if the Appt time for VCV appt was done within CT care recommendation.</description>
    <externalId>false</externalId>
    <formula>IF(Service_Appointment__r.SchedStartTime &lt;= Case_Metrics__r.VCC_Appt_Due_Date__c, TRUE, FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Provided within CT RFI</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
