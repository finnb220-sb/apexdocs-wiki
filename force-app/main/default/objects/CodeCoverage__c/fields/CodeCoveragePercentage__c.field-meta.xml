<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CodeCoveragePercentage__c</fullName>
    <deprecated>false</deprecated>
    <description>The total code coverage percentage for the class or trigger.</description>
    <externalId>false</externalId>
    <formula>IF(TotalLines__c &gt; 0, LinesCovered__c/TotalLines__c, 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The total code coverage percentage for the class or trigger.</inlineHelpText>
    <label>Code Coverage Percentage</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
