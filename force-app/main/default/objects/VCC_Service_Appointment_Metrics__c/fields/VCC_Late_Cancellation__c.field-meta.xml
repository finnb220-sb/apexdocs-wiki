<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VCC_Late_Cancellation__c</fullName>
    <description>Returns true or false if the appointment was cancelled after the appointment was supposed to start</description>
    <externalId>false</externalId>
    <formula>If( Service_Appointment__r.Cancelled_Date__c &gt;  Service_Appointment__r.SchedStartTime, True, False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Late Cancellation</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
