<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingOwnerRules>
        <fullName>Share_Suggestions_From_Specialists_With_Manager</fullName>
        <accessLevel>Read</accessLevel>
        <description>Shares all suggestions made from Specialists to managers</description>
        <label>Share Suggestions From Specialists With Manager</label>
        <sharedTo>
            <group>Provider_Connect_Managers</group>
        </sharedTo>
        <sharedFrom>
            <group>PC_Provider_Connect_Specialists</group>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>View_Only_For_Managers</fullName>
        <accessLevel>Read</accessLevel>
        <description>Shows all suggestion records for Managers so they can accurately view and create reports.</description>
        <label>View Only For Managers</label>
        <sharedTo>
            <group>Provider_Connect_Managers</group>
        </sharedTo>
        <sharedFrom>
            <group>PC_Provider_Connect_PCPs</group>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
