<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TUC_Total_Time__c</fullName>
    <description>Total time spent in minutes in Total</description>
    <externalId>false</externalId>
    <formula>IF(IsClosed, ROUND(((ClosedDate - CreatedDate) *1440),0), ROUND(((NOW() - CreatedDate ) *1440),0))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total time spent in minutes in Total</inlineHelpText>
    <label>Total Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
