<?xml version="1.0" encoding="UTF-8" ?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VCC_Is_My_VISN__c</fullName>
    <description>Returns true or false if the progress note VISN matches with the user&apos;s VISN assignment</description>
    <externalId>false</externalId>
    <formula>IF (VCC_Owner_Division__c = $User.Division ,TRUE, FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is My VISN</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
