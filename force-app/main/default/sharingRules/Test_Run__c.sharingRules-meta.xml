<?xml version="1.0" encoding="UTF-8" ?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Tele_EC_Test_Run_Sharing_Rule</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Tele-EC Test Run Sharing Rule to provide Read/Write access to &quot;Test Run&quot; for the Tele EC Public Group</description>
        <label>Tele-EC Test Run Sharing Rule</label>
        <sharedTo>
            <group>Tele_EC_Public_Group</group>
        </sharedTo>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>notEqual</operation>
            <value />
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
