        </defaultConnector>
        <defaultConnectorLabel>For Yes</defaultConnectorLabel>
        <rules>
            <name>For_No</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>CAPS_lock</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CAPS_lock</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
