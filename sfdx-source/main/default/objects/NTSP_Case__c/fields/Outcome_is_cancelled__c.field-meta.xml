<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Outcome_is_cancelled__c</fullName>
    <description>Hidden field used in reporting. Checks whether or not the value of the Outcome field is Cancelled or not, returns 1 if true and 0 if not.</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL( Outcome__c ,&apos;Cancelled&apos;),1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Outcome is Cancelled</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
