<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Replacement_must_be_ASCII</fullName>
    <active>true</active>
    <errorConditionFormula>Replacement_Character_Code__c &lt; 32 || Replacement_Character_Code__c &gt; 126</errorConditionFormula>
    <errorDisplayField>DeveloperName</errorDisplayField>
    <errorMessage>Character code must be between but not including 31 and 127</errorMessage>
</ValidationRule>
