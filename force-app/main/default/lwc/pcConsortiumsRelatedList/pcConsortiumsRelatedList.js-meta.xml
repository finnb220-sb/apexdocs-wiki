<?xml version="1.0" encoding="UTF-8" ?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>55.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Territory Consortiums</masterLabel>
    <targets>
        <target>lightning__RecordPage</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__RecordPage">
            <property
                name="limitForRecords"
                label="Record Display Limit"
                type="Integer"
                default="5"
                description="Determines the max number of results to display in the related list."
            />
            <property name="iconName" label="Icon Name" type="String" default="custom:custom68" required="true" description="The icon used in the related list component header" />
            <!-- Small form factor required for component to display in salesforce mobile app -->
            <!-- <supportedFormFactors>
                <supportedFormFactor type="Small"/> 
                <supportedFormFactor type="Large"/>
            </supportedFormFactors> -->
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
