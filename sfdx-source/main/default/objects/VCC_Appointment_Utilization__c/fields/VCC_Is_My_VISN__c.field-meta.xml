<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VCC_Is_My_VISN__c</fullName>
    <description>Returns true if the user&apos;s VISN equals the VISN on the appointment record.</description>
    <externalId>false</externalId>
    <formula>IF(VCC_Service_Territory__r.Name = $User.Division, True, False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is My VISN</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
